FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.002";
    object      Ub;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   nonuniform List<vector> 
10
(
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
(0.001 0.001 0.001)
)
;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    top
    {
        type            slip;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    front
    {
        type            cyclic;
    }
    back
    {
        type            cyclic;
    }
}


