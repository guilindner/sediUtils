FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue UbCells (0 0 0)
);

regions
(
    boxToCell
    {
        box (0.00 0.03 0.01) (0.04 0.07 1.0);
        fieldValues
        (
            volVectorFieldValue UbCells (999 999 999)
        );
    }
);


